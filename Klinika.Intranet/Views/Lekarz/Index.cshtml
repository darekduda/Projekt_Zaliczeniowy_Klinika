@model IEnumerable<Klinika.Data.Data.Entities.Lekarz>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Imie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nazwisko)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plec)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adres)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TytulNaukowy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Specjalizacja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Poradnia)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Imie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nazwisko)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plec.IdPlec)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adres.KodPocztowy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TytulNaukowy.IdTytułNaukowy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Specjalizacja.Nazwa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Poradnia.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
